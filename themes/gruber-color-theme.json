		  "punctuation.definition.directive",
		  "keyword.control.directive",
		],
		"settings": {
		  "foreground": "#ffdd33",
		  "fontStyle": ""
		}
	  },
	  {
		"name": "Variables",
		"scope": [
		  "variable",
		  "string constant.other.placeholder"
		],
		"settings": {
		  "foreground": "#f4f4ff"
		}
	  },
	  {
		"name": "Colors",
		"scope": [
		  "constant.other.color"
		],
		"settings": {
		  "foreground": "#ffffff"
		}
